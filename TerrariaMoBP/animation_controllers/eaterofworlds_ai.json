//bridge-file-version: #132
{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.eaterofworlds_ai": {
			"states": {
				"default": {
					"transitions": [
						{
							"choose": "query.life_time>0"
						}
					]
				},
				"choose": {
					"transitions": [
						{
							"move_charge": "math.random(0, 2)>0.1 && math.random(0, 2) <1.8"
						},
						{
							"move_to_player": "math.random(0, 2)> 1.8 && math.random(0, 2) <1.8"
						},
						{
							"circle_player_up": "math.random(0, 2)> 1.8 && math.random(0, 2)< 1.9"
						},
						{
							"wrong_biome": "!query.biome_has_any_tag('corruption')"
						},
						{
							"circle_player_down": "math.random(0, 2)> 1.9"
						}
					]
				},
				"move_charge": {
					"on_entry": [
						"/execute @e[type=terrariamod:eaterofworlds] ~ ~ ~ tp @s ^ ^ ^1",
						"v.time = q.time_stamp;"
					],
					"transitions": [
						{
							"choose": "query.time_stamp>v.time+1"
						}
					]
				},
				"move_to_player": {
					"transitions": [
						{
							"choose": "query.time_stamp>v.time+0.1"
						}
					],
					"on_entry": [
						"/execute @e[type=terrariamod:eaterofworlds] ~ ~ ~ tp @s ^ ^ ^1 facing @p",
						"v.time = q.time_stamp;"
					]
				},
				"circle_player_up": {
					"transitions": [
						{
							"choose": "query.time_stamp>v.time+0.10"
						}
					],
					"on_entry": [
						"/execute @e[type=terrariamod:eaterofworlds] ~ ~ ~ tp @s ^2 ^2 ^2 facing @p",
						"v.time = q.time_stamp;"
					]
				},
				"wrong_biome": {
					"transitions": [
						{
							"delete_mob": "!query.biome_has_any_tag('corruption')"
						},
						{
							"choose": "query.biome_has_any_tag('corruption')"
						}
					],
					"on_entry": [
						"/execute @e[type=terrariamod:eaterofworlds] ~ ~ ~tp @s @p"
					]
				},
				"delete_mob": {
					"transitions": [
						{
							"choose": "query.life_time>0"
						}
					],
					"on_entry": [
						"/kill @e[type=terrariamod:eaterofworlds]"
					]
				},
				"circle_player_down": {
					"transitions": [
						{
							"choose": "query.time_stamp>v.time+0.10"
						}
					],
					"on_entry": [
						"/execute @e[type=terrariamod:eaterofworlds] ~ ~ ~ tp @s ^-2 ^-2 ^-2 facing @p",
						"v.time = q.time_stamp;"
					]
				}
			},
			"initial_state": "default"
		}
	}
}