{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.bridge_nurse.execute_commands": {
			"states": {
				"default": {
					"transitions": [
						{
							"execute_command_id_2": "query.skin_id == 2"
						},
						{
							"execute_command_id_1": "query.skin_id == 1"
						}
					]
				},
				"execute_command_id_1": {
					"transitions": [
						{
							"default": "query.skin_id != 1"
						}
					],
					"on_entry": [
						"/effect @p instant_health 9 9",
						"/clear @p terrariamod:coppercoin 0 9",
						"/say \"That didn't hurt to bad did it?\"",
						"@s bridge:remove_command_id_1"
					]
				},
				"execute_command_id_2": {
					"transitions": [
						{
							"default": "query.skin_id != 2"
						}
					],
					"on_entry": [
						"/effect @p instant_health 9 9",
						"/clear @p terrariamod:coppercoin 0 9",
						"/say \"All better. I don't want to see you jumping off anymore cliffs.\"?\"",
						"@s bridge:remove_command_id_2"
					]
				}
			}
		}
	}
}